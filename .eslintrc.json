{
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "avoid",
        "bracketSameLine": true,
        "singleQuote": true,
        "trailingComma": "all",
        "endofline": "auto",
        "bracketSpacing": true
      }
    ]
  },
  "extends": ["plugin:prettier/recommended"]
}
